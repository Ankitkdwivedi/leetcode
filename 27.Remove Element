class Solution {
    public int removeElement(int[] nums, int val) {
            int i,c=0;
            for(i=0;i<nums.length;i++){
                if(nums[i]==val){
                        continue;
                }
                    else{
                        nums[c]=nums[i];
                            c++;
                    }
            }
            return c;
        
    }
}
